{
    "name": "vngn",
    "version": "0.1.0",
    "description": "Application starter",
    "type": "module",
    "scripts": {
        "build": "rimraf build && tsc && tsc-alias",
        "build-production": "rimraf build && tsc -p tsconfig.production.json && tsc-alias",
        "prettier": "prettier \"**/*.{yml,yaml,json,md}\" --write",
        "lint": "eslint --fix --max-warnings 0 \"!(node_modules)/**/*.{mts,ts}\"",
        "test": "rimraf build && tsc && tsc-alias && c8 --include build/ mocha 'build/tests/**/*.test.mjs'",
        "test:unit": "rimraf build && tsc && tsc-alias && c8 --include build/ mocha 'build/tests/unit/**/*.test.mjs'",
        "test:unit:infrastructure-dependencies": "rimraf build && tsc && tsc-alias && c8 --include build/ mocha 'build/tests/unit/infrastructure/dependencies/**/*.test.mjs'",
        "test:unit:infrastructure-interaction": "rimraf build && tsc && tsc-alias && c8 --include build/ mocha 'build/tests/unit/infrastructure/interaction/**/*.test.mjs'",
        "prepare": "husky install",
        "pre-commit": "npm run prettier && npm run lint && npm run test"
    },
    "author": "Andrei Piontkovskii",
    "devDependencies": {
        "@databases/mysql-test": "^4.0.2",
        "@databases/pg-test": "^3.1.2",
        "@types/chai": "^4.3.1",
        "@types/command-line-args": "^5.2.0",
        "@types/command-line-usage": "^5.0.2",
        "@types/common-tags": "^1.8.1",
        "@types/mocha": "^9.1.1",
        "@types/node": "^17.0.30",
        "@types/node-schedule": "^2.1.0",
        "@types/sinon": "^10.0.11",
        "@types/ws": "^8.5.3",
        "@typescript-eslint/eslint-plugin": "^5.30.3",
        "@typescript-eslint/parser": "^5.21.0",
        "c8": "^7.11.0",
        "chai": "^4.3.6",
        "eslint": "^8.14.0",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^8.0.1",
        "mocha": "^10.0.0",
        "prettier": "^2.6.2",
        "sinon": "^13.0.1",
        "tsc-alias": "^1.6.7",
        "tslib": "^2.4.0",
        "typescript": "^4.6.4"
    },
    "dependencies": {
        "@databases/mysql": "^5.2.1",
        "@databases/pg": "^5.4.1",
        "@databases/sql": "^3.2.0",
        "@sentry/integrations": "^6.19.7",
        "@sentry/node": "^6.19.7",
        "@sentry/tracing": "^6.19.7",
        "ajv": "^8.11.0",
        "chalk": "^5.0.1",
        "command-line-args": "^5.2.1",
        "command-line-usage": "^6.1.3",
        "common-tags": "^1.8.2",
        "node-schedule": "^2.1.0",
        "openapi3-ts": "^2.0.2",
        "path-parser": "^6.1.0",
        "redis": "^4.1.0",
        "string-argv": "^0.3.1",
        "ts-essentials": "^9.1.2",
        "winston": "^3.7.2",
        "ws": "^8.6.0"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.6",
        "utf-8-validate": "^5.0.9"
    }
}
